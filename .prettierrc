{
  "semi": true,
  "useTabs": false,
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@framework",
    "^@framework/(.*)$",
    "^@database",
    "^@database/(.*)$",
    "^@utils",
    "^@utils/(.*)$",
    "^@constants",
    "^@types",
    "^@types/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
